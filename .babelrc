{
  "presets": [
    "babel-preset-expo"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ],
        "alias": {
          "@components": "./src/components",
          "@screens": "./src/screens",
          "@store": "./src/store",
          "@utils": "./src/utils"
        }
      }
    ]
  ],
  "env": {
    "development": {
      "plugins": [
        "transform-react-jsx-source"
      ]
    }
  }
}
